							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"id\": 1\n}"
						},
						"url": {
							"raw": "{{base_url}}/rekenings",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"rekenings"
