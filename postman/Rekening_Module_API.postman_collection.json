{
	"info": {
		"_postman_id": "rekening-module-api-collection",
		"name": "EASB - Rekening Module API",
		"description": "API collection for Rekening Module - managing rekening data.",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Rekening Management (SUPERADMIN Only)",
			"item": [
				{
					"name": "Create Rekening",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{access_token}}",
								"description": "JWT token for SUPERADMIN role"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"rekening_kode\": \"1.1.1.01\",\n  \"rekening_uraian\": \"Kas Daerah\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/rekenings",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"rekenings"
							]
						},
						"description": "Create a new Rekening entry. Requires SUPERADMIN role.\n\n**Fields:**\n- `rekening_kode` (string, required): Code of the rekening\n- `rekening_uraian` (string, required): Description of the rekening"
					},
					"response": []
				},
				{
					"name": "Update Rekening",
					"request": {
						"method": "PUT",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{access_token}}",
								"description": "JWT token for SUPERADMIN role"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"id\": 1,\n  \"rekening_kode\": \"1.1.1.01\",\n  \"rekening_uraian\": \"Kas Daerah Updated\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/rekenings",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"rekenings"
							]
						},
						"description": "Update an existing Rekening. Requires SUPERADMIN role.\n\n**Fields:**\n- `id` (number, required): ID of the rekening\n- `rekening_kode` (string, required): Code of the rekening\n- `rekening_uraian` (string, required): Description of the rekening"
					},
					"response": []
