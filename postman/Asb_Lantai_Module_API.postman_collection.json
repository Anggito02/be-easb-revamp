							},
							{
								"key": "Authorization",
								"value": "Bearer {{access_token}}",
								"description": "JWT token for SUPERADMIN role"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"id\": 1\n}"
						},
