	},
	"item": [
		{
			"name": "ASB Lantai Management (SUPERADMIN Only)",
			"item": [
				{
					"name": "Create ASB Lantai",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{access_token}}",
								"description": "JWT token for SUPERADMIN role"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"lantai\": \"Lantai 1\",\n  \"type\": \"Standard\",\n  \"koef\": 1.0,\n  \"id_satuan\": 1\n}"
						},
						"url": {
							"raw": "{{base_url}}/asb-lantai",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"asb-lantai"
							]
						},
						"description": "Create a new ASB Lantai entry. Requires SUPERADMIN role.\n\n**Fields:**\n- `lantai` (string, required): Name of the lantai\n- `type` (string, required): Type of the lantai\n- `koef` (number, required): Coefficient value\n- `id_satuan` (number, required): ID of the satuan"
					},
					"response": []
				},
				{
					"name": "Update ASB Lantai",
					"request": {
						"method": "PUT",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{access_token}}",
								"description": "JWT token for SUPERADMIN role"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"id\": 1,\n  \"lantai\": \"Lantai 1 Updated\",\n  \"type\": \"Premium\",\n  \"koef\": 1.2,\n  \"id_satuan\": 1\n}"
						},
						"url": {
							"raw": "{{base_url}}/asb-lantai",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"asb-lantai"
							]
						},
						"description": "Update an existing ASB Lantai. Requires SUPERADMIN role.\n\n**Fields:**\n- `id` (number, required): ID of the lantai\n- `lantai` (string, optional): Name of the lantai\n- `type` (string, optional): Type of the lantai\n- `koef` (number, optional): Coefficient value\n- `id_satuan` (number, optional): ID of the satuan"
					},
					"response": []
				},
